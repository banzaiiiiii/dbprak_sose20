<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojos.University" table="university" schema="public" catalog="socialnetwork">
        <id name="universityId" column="university_id"/>
        <property name="universityOrganisationId" column="university_organisation_id"/>
        <property name="universityCityId" column="university_city_id"/>
    </class>
</hibernate-mapping>