<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojos.MessageHasTag" table="message_has_tag" schema="public" catalog="socialnetwork">
        <composite-id mapped="true" class="com.hibernate.pojos.MessageHasTagPK">
            <key-property name="messageHasTagMessageId" column="message_has_tag_message_id"/>
            <key-property name="messageHasTagTagId" column="message_has_tag_tag_id"/>
        </composite-id>
    </class>
</hibernate-mapping>