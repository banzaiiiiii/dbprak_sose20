<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojos.City" table="city" schema="public" catalog="socialnetwork">
        <id name="cityId" column="city_id"/>
        <property name="cityPlaceId" column="city_place_id"/>
        <property name="cityCountryId" column="city_country_id"/>
    </class>
</hibernate-mapping>