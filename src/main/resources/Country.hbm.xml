<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojos.Country" table="country" schema="public" catalog="socialnetwork">
        <id name="countryId" column="country_id"/>
        <property name="countryPlaceId" column="country_place_id"/>
        <property name="countryContinentId" column="country_continent_id"/>
    </class>
</hibernate-mapping>