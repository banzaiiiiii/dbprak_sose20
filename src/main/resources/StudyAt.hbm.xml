<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojos.StudyAt" table="study_at" schema="public" catalog="socialnetwork">
        <composite-id mapped="true" class="com.hibernate.pojos.StudyAtPK">
            <key-property name="studyAtPersonId" column="study_at_person_id"/>
            <key-property name="studyAtUniversityId" column="study_at_university_id"/>
            <key-property name="studyAtClassYear" column="study_at_class_year"/>
        </composite-id>
    </class>
</hibernate-mapping>