<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojos.Comment" table="comment" schema="public" catalog="socialnetwork">
        <id name="commentId" column="comment_id"/>
        <property name="commentMessageId" column="comment_message_id"/>
        <property name="commentCommentedMessageId" column="comment_commented_message_id"/>
    </class>
</hibernate-mapping>