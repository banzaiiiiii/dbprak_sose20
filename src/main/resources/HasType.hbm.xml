<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojos.HasType" table="has_type" schema="public" catalog="socialnetwork">
        <composite-id mapped="true" class="com.pojos.HasTypePK">
            <key-property name="tagId" column="tag_id"/>
            <key-property name="tagClassId" column="tag_class_id"/>
        </composite-id>
    </class>
</hibernate-mapping>