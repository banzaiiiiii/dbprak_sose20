<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojos.Knows" table="knows" schema="public" catalog="socialnetwork">
        <composite-id mapped="true" class="com.hibernate.pojos.KnowsPK">
            <key-property name="knowsPersonId" column="knows_person_id"/>
            <key-property name="knowsOtherPersonId" column="knows_other_person_id"/>
        </composite-id>
        <property name="knowsCreationDate" column="knows_creation_date"/>
    </class>
</hibernate-mapping>