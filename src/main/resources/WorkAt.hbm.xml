<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.pojos.WorkAt" table="work_at" schema="public" catalog="socialnetwork">
        <composite-id mapped="true" class="com.hibernate.pojos.WorkAtPK">
            <key-property name="workAtPersonId" column="work_at_person_id"/>
            <key-property name="workAtCompanyId" column="work_at_company_id"/>
            <key-property name="workAtWorkFrom" column="work_at_work_from"/>
        </composite-id>
    </class>
</hibernate-mapping>